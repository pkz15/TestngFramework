<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RegressionSuite" parallel="tests" thread-count="3">
    
   

    <!-- Listener for ExtentReports -->
    <listeners>
        <listener class-name="com.framework.base.TestListener"/>
    </listeners>

    <test name="ContactUsTest_Edge">
      <parameter name="browser" value="chrome" />
      
        <classes>
            <class name="com.framework.tests.contactUsTestPage"/>
        </classes>
    </test>
     <test name="SearchTest_Chrome">
      
        <classes>
            <class name="com.framework.tests.contactUsTestPage"/>
        </classes>
    </test>

    <!-- Test 3 -->
    <test name="CheckoutTest_Firefox">
     <parameter name="browser" value="chrome" />
        <classes>
            <class name="com.framework.tests.contactUsTestPage"/>
        </classes>
    </test>

</suite>
